{
  "name": "Blue Ocean Description HTMLify",
  "version": "1.3",
  "description": "Turn Jenkins Blue Ocean Descriptions into proper HTML!",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://*/blue/*"],
      "js": ["htmlify.js"],
      "css": ["htmlify.css"]
    }
  ],
  "icons": { "128": "ship_emoji.png" },
  "action": {}
}
